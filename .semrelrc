{
    "plugins": {
        "commit-analyzer": {
            "name": "default@^1.12.0"
        },
        "ci-condition": {
            "name": "github@^1.9.0",
            "options": {
                "defaultBranch": "master"
            }
        },
        "changelog-generator": {
            "name": "default@1.13.0",
            "options": {
                "emojis": "true"
            }
        },
        "provider": {
            "name": "github@^1.15.0",
            "options": {
                "github_use_compare_commits": true
            }
        }
    }
}